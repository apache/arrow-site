        <name>CU_RETURN_NOT_OK</name>
        <param><defname>STMT</defname></param>
        <initializer>do {                                                                          \
    CUresult ret = (STMT);                                                      \
    if (ret != CUDA_SUCCESS) {                                                  \
      const char* name;                                                         \
      const char* description;                                                  \
      cuGetErrorName(ret, &amp;name);                                               \
      cuGetErrorString(ret, &amp;description);                                      \
      return Status::IOError(&quot;Cuda Driver API call in &quot;, __FILE__, &quot; at line &quot;, \
                             __LINE__, &quot; failed with &quot;, name, &quot;[&quot;, ret, &quot;] (&quot;,  \
                             description, &quot;): &quot;, #STMT);                        \
    }                                                                           \
  } while (0)</initializer>
